{
  "name": "@keg-hub/keg-core",
  "main": "node_modules/expo/AppEntry.js",
  "version": "8.0.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "git+https://github.com/simpleviewinc/keg-hub.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/simpleviewinc/keg-hub/issues"
  },
  "homepage": "https://github.com/simpleviewinc/keg-hub#readme",
  "license": "MIT",
  "scripts": {
    "android": "EXPO_DEBUG=true NODE_ENV=development PLATFORM=native expo start -clear --android --dev",
    "build": "echo \"Keg-Core build!\"",
    "build:android": "NODE_ENV=production PLATFORM=native expo build:android",
    "build:ios": "NODE_ENV=production PLATFORM=native expo build:ios",
    "build:web": "yarn clean:web:build && NODE_ENV=production PLATFORM=web expo build:web --no-pwa",
    "clean": "yarn clean:cache && yarn clean:nm",
    "clean:babel": "rimraf ./node_modules/.cache/@babel",
    "clean:cache": "yarn clean:rn && yarn clean:babel; yarn cache clean",
    "clean:dev": "yarn clean:rn; yarn clean:babel; yarn clean:web:build; yarn clean:nm",
    "clean:full": "rimraf yarn.lock && yarn clean:dev",
    "clean:install": "yarn clean:full; yarn install --force",
    "clean:nm": "rimraf yarn.lock; rimraf package.lock; rimraf node_modules",
    "clean:rn": "yarn cache clean && watchman watch-del-all && rimraf $TMPDIR/react-* && rimraf $TMPDIR/metro-bundler-cache-*",
    "clean:web:build": "rimraf ./web-build",
    "eslint:watch": "onchange '**/*.{js,jsx}' -- eslint --config ./core/configs/eslintrc.config.js {{changed}} --fix",
    "format:eslint": "eslint --config ./core/configs/eslintrc.config.js . --fix --quiet",
    "format:prettier": "prettier --config ./core/configs/prettier.config.js --ignore-path .eslintignore --write '**/*.{js,jsx}'",
    "format": "yarn format:prettier && yarn format:eslint",
    "ios": "EXPO_DEBUG=true NODE_ENV=development PLATFORM=native expo start --clear --ios --dev",
    "postinstall": "node ./core/scripts/js/postInstall.js",
    "roll:dev": "PLATFORM=web rollup -c ./core/configs/rollup.config.js",
    "start": "NODE_ENV=development expo start --clear",
    "tap:start": "nodemon --watch ../../package.json --watch ./package.json --watch ../../configs --watch ./core/configs  --exec 'EXPO_DEBUG=true NODE_ENV=development PLATFORM=web expo start --clear --web --dev'",
    "test": "PLATFORM=web NODE_ENV=test jest --no-cache --config=./core/configs/jest.config.js",
    "test:reports": "yarn test --coverage && yarn view:reports",
    "view:reports": "open reports/test-results.html && open reports/coverage/lcov-report/index.html",
    "web": "nodemon --watch ./core/configs --watch ./package.json --exec 'EXPO_DEBUG=true NODE_ENV=development PLATFORM=web expo start --clear --web --dev'",
    "print:envs": "node core/scripts/js/printKegEnvs.js ./"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-transform-destructuring": "7.12.1",
    "@babel/plugin-transform-parameters": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "@babel/runtime": "7.12.1",
    "@expo/webpack-config": "0.12.16",
    "@keg-hub/jsutils": "8.0.0",
    "@keg-hub/keg-components": "8.0.0",
    "@keg-hub/re-theme": "8.0.0",
    "@keg-hub/tap-resolver": "7.0.0",
    "@testing-library/react-hooks": "3.2.1",
    "app-root-path": "3.0.0",
    "axios": "0.21.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-preset-env": "1.7.0",
    "babel-preset-expo": "8.3.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.5.0",
    "eslint": "7.4.0",
    "eslint-plugin-jest": "23.18.0",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-native": "3.8.1",
    "expo": "39.0.0",
    "expo-secure-store": "9.2.0",
    "fs-extra": "8.1.0",
    "husky": "4.2.5",
    "jest-expo": "39.0.0",
    "jest-html-reporter": "3.1.3",
    "lint-staged": "10.2.10",
    "moment": "2.27.0",
    "moment-timezone": "0.5.31",
    "nodemon": "2.0.4",
    "onchange": "7.0.2",
    "prettier": "2.0.5",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "0.63.3",
    "react-native-linear-gradient": "2.5.6",
    "react-native-screens": "2.10.1",
    "react-native-svg": "12.1.0",
    "react-native-svg-web": "1.0.9",
    "react-native-web": "0.14.7",
    "react-native-web-linear-gradient": "1.1.1",
    "react-redux": "7.2.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-router-native": "5.2.0",
    "react-test-renderer": "16.13.1",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "rimraf": "3.0.2",
    "rollup": "2.23.1"
  },
  "nodemonConfig": {
    "ext": "js",
    "ignore": [
      "/keg/tap/src",
      "/keg/tap/node_modules/*",
      "/keg/tap/node_modules/keg-core/node_modules/*",
      "/keg/tap/node_modules/keg-core/core"
    ],
    "watch": [
      "/keg/tap/configs",
      "/keg/tap/package.json",
      "/keg/tap/node_modules/keg-core/package.json",
      "/keg/tap/node_modules/keg-core/core/configs"
    ],
    "delay": "2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --config ./core/configs/lint-staged.config.js"
    }
  }
}
