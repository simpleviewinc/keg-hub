{
  "name": "SVKeg",
  "displayName": "SVKeg",
  "expo": {
    "name": "kegexpo",
    "slug": "kegexpo",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  },
  "keg": {
    "name": "SVKeg",
    "routes": {},
    "overrides": {
      "kegName": [
        "keg",
        "name"
      ],
      "kegSrc": [
        "keg",
        "tapResolver",
        "paths",
        "kegSrc"
      ]
    },
    "paths": {
      "buildConfig": "core/configs/build.config.js",
      "exportFolder": "builds"
    },
    "tapResolver": {
      "paths": {
        "temp": "./temp",
        "kegSrc": "./core/base",
        "tapAssets": "assets",
        "folderRootFile": "index"
      },
      "extensions": {
        "assets": [
          ".png",
          ".jpg",
          ".jpeg",
          ".gif",
          ".ttf"
        ],
        "resolve": [
          ".web.js",
          ".native.js",
          ".ios.js",
          ".android.js",
          ".js",
          ".json",
          ".sqlite",
          ".ttf"
        ]
      },
      "aliases": {
        "nameSpace": "SV",
        "root": {
          "AppConfig": "app.json",
          "Base": "core/base",
          "Core": "core",
          "Mocks": "core/mocks"
        },
        "base": {
          "Daos": "daos",
          "Native": "native",
          "Store": "store"
        },
        "dynamic": {
          "Actions": "actions",
          "Assets": "assets",
          "Components": "components",
          "Containers": "containers",
          "Constants": "constants",
          "Navigation": "navigation",
          "Models": "models",
          "Reducers": "reducers",
          "Base": "",
          "Services": "services",
          "Screens": "containers/screens",
          "Store": "store",
          "Theme": "theme",
          "Utils": "utils",
          "Hooks": "hooks"
        },
        "web": {
          "^react-native$": "react-native-web",
          "^react-native-svg": "react-native-svg-web",
          "react-router-native": "react-router-dom"
        },
        "native": {}
      },
      "babel": {
        "web": {
          "presets": [
            "babel-preset-expo"
          ],
          "plugins": [
            [
              "react-native-web"
            ]
          ]
        },
        "native": {
          "presets": [
            "babel-preset-expo"
          ],
          "plugins": []
        }
      }
    },
    "versions": {
      "development": "1.0.0",
      "production": "1.0.0",
      "qa": "1.0.0",
      "staging": "1.0.0"
    }
  }
}
