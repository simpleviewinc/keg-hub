{
  "name": "keg-cli",
  "displayName": "Keg CLI",
  "docker": {
    "providerUrl": "docker.pkg.github.com",
    "namespace": "simpleviewinc/keg-packages",
    "user": "",
    "token": ""
  },
  "cli": {
    "paths": {
      "customTasks": ""
    },
    "git": {
      "orgName": "simpleviewinc",
      "orgUrl": "https://github.com/simpleviewinc",
      "provider": "https://github.com",
      "publicToken": "c9ee22e12f21993e6cbe0add9319bf59:b15873b3f06436e89b0cb652f4bc07590876a22017946b1dad13d5772349e32d5df307096f379f262d3afaaf6d2c4b73",
      "repos": {
        "hub": "keg-hub",
        "rc": "tap-release-client"
      }
    },
    "taps": {
      "links": {
      }
    },
    "settings": {
      "defaultEnv": "development",
      "docker": {
        "preConfirm": true,
        "buildKit": true,
        "force": true,
        "defaultLocalBuild": true
      },
      "checkUpdates": false,
      "editorCmd": "code",
      "errorStack": false,
      "git": {
        "secure": false
      },
      "task": {
        "optionsAsk": true
      }
    }
  },
  "publish": {
    "default": {
      "tasks": {
        "install": true,
        "test": true,
        "build": true,
        "version": true,
        "publish": false,
        "git": false
      }
    },
    "keg": {
      "name": "keg",
      "dependent": true,
      "order": [
        "@keg-hub/re-theme",
        "@keg-hub/keg-components",
        "@keg-hub/keg-core"
      ]
    },
    "retheme": {
      "name": "retheme",
      "dependent": false,
      "order": [
        "@keg-hub/re-theme"
      ]
    },
    "components": {
      "name": "components",
      "dependent": false,
      "order": [
        "@keg-hub/keg-components"
      ]
    },
    "core": {
      "name": "core",
      "dependent": false,
      "order": [
        "@keg-hub/keg-core"
      ]
    }
  }
}
